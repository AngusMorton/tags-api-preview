{
  "name": "@marko/tags-api-preview",
  "description": "A glimpse into the future of Marko.",
  "version": "0.0.3",
  "author": "Dylan Piercey <dpiercey@ebay.com>",
  "bugs": "https://github.com/marko-js/tags-api-preview/issues",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@marko/compiler": "^5.14.1",
    "@marko/fixture-snapshots": "^2.1.1",
    "@marko/testing-library": "^5.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/babel__code-frame": "^7.0.3",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/chai-dom": "0.0.10",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.3",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.9.0",
    "codecov": "^3.8.3",
    "esbuild": "^0.12.16",
    "esbuild-register": "^2.6.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "fixpack": "^4.0.0",
    "globby": "^11.0.4",
    "husky": "^7.0.1",
    "jsdom-context-require": "^3.1.0",
    "lint-staged": "^11.1.1",
    "marko": "^5.14.1",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "standard-version": "^9.3.1",
    "typescript": "^4.3.5"
  },
  "files": [
    "dist",
    "marko.json",
    "!**/test",
    "!*.tsbuildinfo"
  ],
  "homepage": "https://github.com/marko-js/tags-api-preview",
  "keywords": [
    "api",
    "marko",
    "preview",
    "tags"
  ],
  "license": "MIT",
  "main": "marko.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js/tags-api-preview"
  },
  "scripts": {
    "build": "tsc && node -r esbuild-register build",
    "ci:report": "codecov",
    "ci:test": "NODE_ENV=test nyc npm run mocha -- --forbid-pending --forbid-only",
    "format": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write && (fixpack || true)",
    "lint": "tsc && npm run lint:eslint && npm run lint:prettier -- -l && fixpack",
    "lint:eslint": "eslint -f visualstudio .",
    "lint:prettier": "prettier '**/*{.ts,.js,.json,.md,.yml,rc}'",
    "mocha": "mocha 'src/**/*.test.ts'",
    "postpublish": "sed -i '' 's/dist/src/' marko.json",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && sed -i '' 's/src/dist/' marko.json",
    "release": "standard-version",
    "report": "open ./coverage/lcov-report/index.html",
    "test": "NODE_ENV=test npm run mocha -- --watch",
    "test:inspect": "npm test -- --inspect"
  }
}
